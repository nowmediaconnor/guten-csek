{
    "name": "guten-csek",
    "version": "1.0.0",
    "description": "A series of Gutenberg blocks for Csek Creative's rebrand.",
    "main": "build/index.js",
    "author": "Connor Doman",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "wp-scripts start",
        "build": "wp-scripts build",
        "count": "python count_lines.py src -i img,fonts",
        "watch:css-app": "cross-env NODE_ENV=development tailwindcss -i ./src/css/app.css -o ./css/guten-csek.css --postcss --watch",
        "watch:css-editor": "cross-env NODE_ENV=development tailwindcss -i ./src/css/app-editor.css -o ./css/guten-csek-editor.css --postcss --watch",
        "watch": "cross-env NODE_ENV=development concurrently \"yarn watch:css-app\" \"yarn watch:css-editor\" \"yarn start\""
    },
    "devDependencies": {
        "@jeffreyvr/tailwindcss-tailpress": "^2.0.0",
        "@types/wordpress__block-editor": "^11.5.1",
        "@types/wordpress__components": "^23.0.1",
        "@wordpress/block-editor": "^12.7.0",
        "@wordpress/components": "^25.5.0",
        "@wordpress/scripts": "^26.10.0",
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.1",
        "cross-env": "^7.0.3",
        "esbuild": "^0.19.4",
        "eslint-plugin-react-hooks": "^4.6.0",
        "postcss": "^8.4.31",
        "postcss-import": "^15.1.0",
        "postcss-nested-ancestors": "^3.0.0",
        "tailwindcss": "^3.3.3"
    },
    "dependencies": {
        "circletype": "^2.3.0",
        "tailwind-merge": "^1.14.0",
        "typescript-cubic-spline": "^1.0.1"
    }
}
