{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "title": "Csek Scrolling Projects Block",
    "description": "A block for showcasing a few links and images to projects.",
    "category": "csek",
    "name": "guten-csek/scrolling-projects",
    "icon": "expand",
    "keywords": ["csek", "expanding", "media", "blockquote", "quote"],
    "textdomain": "guten-csek",
    "attributes": {
        "projects": {
            "type": "array",
            "default": []
        }
    },
    "editorScript": "file:./index.js",
    "editorStyle": "file:./editor.css",
    "style": "file:./style.css",
    "viewScript": "file:./controller.js"
}
